:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: system-ui;
	--theme-font-family-heading: system-ui;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 9999px;
	--theme-rounded-container: 8px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: 255 255 255;
	--on-secondary: 255 255 255;
	--on-tertiary: var(--color-primary-500);
	--on-success: 0 0 0;
	--on-warning: 255 255 255;
	--on-error: 0 0 0;
	--on-surface: 255 255 255;
	/* =~= Theme Colors  =~= */
	/* primary | #0D1321 */
	--color-primary-50: 219 220 222; /* ⬅ #dbdcde */
	--color-primary-100: 207 208 211; /* ⬅ #cfd0d3 */
	--color-primary-200: 195 196 200; /* ⬅ #c3c4c8 */
	--color-primary-300: 158 161 166; /* ⬅ #9ea1a6 */
	--color-primary-400: 86 90 100; /* ⬅ #565a64 */
	--color-primary-500: 13 19 33; /* ⬅ #0D1321 */
	--color-primary-600: 12 17 30; /* ⬅ #0c111e */
	--color-primary-700: 10 14 25; /* ⬅ #0a0e19 */
	--color-primary-800: 8 11 20; /* ⬅ #080b14 */
	--color-primary-900: 6 9 16; /* ⬅ #060910 */
	/* secondary | #FC6471 */
